<?xml version="1.0" encoding="UTF-8"?>
<Layout
  xmlns="urn:speedata.de:2009/publisher/en"
  xmlns:sd="urn:speedata:2009/publisher/functions/en">
  <Pageformat width="70mm" height="30mm"/>
  <Stylesheet>
    green {
      color: green;
    }
    .blue {
      color: blue;
    }
    .bgcolor {
       background-color: red;
       background-padding-top: -1pt;
       background-padding-bottom: -1pt;
       }
  </Stylesheet>

  <Record element="data">
    <PlaceObject>
      <Textblock>
        <Paragraph>
          <Value select="."/>
        </Paragraph>
      </Textblock>
    </PlaceObject>
  </Record>
</Layout>
