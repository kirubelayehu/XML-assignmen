      <xs:attribute name="author">
        <xs:annotation>
          <xs:documentation>Legt den Autor des Dokuments fest</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="duplex">
        <xs:annotation>
          <xs:documentation>Setze PDF Anzeigeprogramm auf einseitigen oder doppelseitigen Druck. Voreinstellung: leer.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="simplex">
              <xs:annotation>
                <xs:documentation>Eine Seite je Blatt</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="duplexflipshortedge">
              <xs:annotation>
                <xs:documentation>Beidseitig und an kurzer Kante umblättern</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="duplexfliplongedge">
              <xs:annotation>
                <xs:documentation>Beidseitig und an langer Kante umblättern</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="keywords">
        <xs:annotation>
          <xs:documentation>Legt die Schlüsselwörter des Dokuments fest (kommaseparierte Liste)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="numcopies">
        <xs:annotation>
          <xs:documentation>Setze Anzahl der Kopien. Maximal erlaubt sind 5 aufgrund der PDF-Spezifikation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="picktraybypdfsize">
        <xs:annotation>
          <xs:documentation>Auswahlfeld im PDF-Viewer »Papierquelle gemäß PDF-Seitengröße auswählen«.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Checkbox aktivieren</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Checkbox deaktivieren</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="printscaling">
        <xs:annotation>
          <xs:documentation>Soll der Drucker die Seiten skalieren?</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="appdefault">
              <xs:annotation>
                <xs:documentation>Voreinstellung des PDF-Viewers benutzen</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="none">
              <xs:annotation>
                <xs:documentation>Keine Seitenskalierung</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="subject">
        <xs:annotation>
          <xs:documentation>Legt den Betreff (subject) des Dokuments fest</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="title">
        <xs:annotation>
          <xs:documentation>Legt den Titel des Dokuments fest</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="PlaceObject">
    <xs:annotation>
      <xs:documentation>Gibt ein rechteckiges Objekt (Bild, Tabelle, Box, Barcode oder Textblock) aus.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Box"/>
        <xs:element ref="en:Circle"/>
        <xs:element ref="en:Rule"/>
        <xs:element ref="en:Textblock"/>
        <xs:element ref="en:Barcode"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:Transformation"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:Switch"/>
      </xs:choice>
      <xs:attribute name="allocate">
        <xs:annotation>
          <xs:documentation>Bestimmt, ob im Raster der Platz für das Objekt als »belegt« markiert wird. Bei allocate="no" wird auch die aktuelle Zeile im Raster nicht verändert.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Platz im Raster reservieren (Voreinstellung für Platzierung im Raster).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Bereich nicht als Belegt markieren (Voreinstellung für absolute Positionierung)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="allocate-left">
        <xs:annotation>
          <xs:documentation>Zusätzliche Belegung links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="allocate-right">
        <xs:annotation>
          <xs:documentation>Zusätzliche Belegung rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="allocate-top">
        <xs:annotation>
          <xs:documentation>Zusätzliche Belegung oben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="allocate-bottom">
        <xs:annotation>
          <xs:documentation>Zusätzliche Belegung unten.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="area">
        <xs:annotation>
          <xs:documentation>Name des Bereichs (Platzierungsbereich), in dem das Objekt ausgegeben werden soll. Wird kein Bereich angegeben, so wird das Objekt auf der »Seite« (dem Bereich, der alle Rasterkästen beinhaltet) ausgegeben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background">
        <xs:annotation>
          <xs:documentation>Erzeugt einen farbigen Hintergrund für das Objekt. Das Attribut ›backgroundcolor‹ gibt die Farbe an.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="full">
                  <xs:annotation>
                    <xs:documentation>Den Hintergrund in der angegebenen Farbe füllen (in backgroundcolor)</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="without">
                  <xs:annotation>
                    <xs:documentation>Leerer Hintergrund</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Hintergrundfarbe wenn das Objekt mit einer Farbe hinterlegt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom-right-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke unten rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top-right-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke oben rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top-left-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke oben links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom-left-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke unten links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="column">
        <xs:annotation>
          <xs:documentation>Zahl: Die Spalte im Raster, in der das Objekt ausgegeben wird. Wird die Spalte nicht angegeben, so sucht sich das System selbständig eine passende freie Zeile für das Objekt. Maßangabe: die Position von der linken Blattkante.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="frame">
        <xs:annotation>
          <xs:documentation>Es wird ein Rahmen um das Objekt gezeichnet. Zusätzlich muss die Rahmenfarbe angegeben werden.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="solid">
              <xs:annotation>
                <xs:documentation>Einen Rahmen um das Objekt zeichnen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="without">
              <xs:annotation>
                <xs:documentation>Keinen Rahmen zeichnen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="framecolor">
        <xs:annotation>
          <xs:documentation>Die Farbe des Rahmens, wenn ein Rahmen ausgegeben wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="groupname">
        <xs:annotation>
          <xs:documentation>Wenn eine Gruppe ausgegeben werden soll, muss der Name hier angegeben werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="hreference">
        <xs:annotation>
          <xs:documentation>Bestimmt, ob die angegebene Spalte die linke oder die rechte Kante vom Objekt bezeichnet.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="left">
              <xs:annotation>
                <xs:documentation>Das Objekt wird in der angegebenen Spalte platziert.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="right">
              <xs:annotation>
                <xs:documentation>Das Objekt wird mit dem rechten Rand in der gegebenen Spalte ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="halign">
        <xs:annotation>
          <xs:documentation>Wird ein Objekt ausgegeben, das kein vielfaches der Rasterbreite ist, dann gibt das Attribut die horizontale Ausrichtung an. ›left‹ beispielsweise bedeutet, dass der freie Platz rechts des Objekts bleibt. Voreinstellung ist ›left‹.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="left">
                  <xs:annotation>
                    <xs:documentation>Das Objekt wird links ausgerichtet.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="center">
                  <xs:annotation>
                    <xs:documentation>Das Objekt wird so platziert, dass der Leerraum links und rechts derselbe ist.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="right">
                  <xs:annotation>
                    <xs:documentation>Das Objekt wird rechts ausgerichtet.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="keepposition">
        <xs:annotation>
          <xs:documentation>Den virtuellen Cursor nicht verändern.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Den virtuellen Cursor nicht verändern.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Den virtuellen Cursor verschieben (Voreinstellung)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="maxheight">
        <xs:annotation>
          <xs:documentation>Die maximale Höhe in Rasterzellen, die das Objekt belegen darf. Wird nur für den automatischen Tabellenumbruch benötigt. Die umbrochenen Tabellen auf den Folgeseiten haben maximal die angegebene Höhe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="origin-x">
        <xs:annotation>
          <xs:documentation>Der Ursprung für die Drehung</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="left">
              <xs:annotation>
                <xs:documentation>Der Ursprung liegt links.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="center">
              <xs:annotation>
                <xs:documentation>Der Ursprung liegt im Mittelpunkt des Objekts.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="right">
              <xs:annotation>
                <xs:documentation>Der Ursprung liegt rechts.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="origin-y">
        <xs:annotation>
          <xs:documentation>Der Ursprung der Drehung (vertikale Achse)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="top">
              <xs:annotation>
                <xs:documentation>Rotation um die obere Kante des Objekts.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="center">
              <xs:annotation>
                <xs:documentation>Rotation um die vertikale Mitte.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bottom">
              <xs:annotation>
                <xs:documentation>Um einen Punkt am unteren Rand rotieren.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="page">
        <xs:annotation>
          <xs:documentation>Die Seite (später im PDF), auf der das Objekt erscheinen soll. Angabe als Zahl oder »next« für die nächste Seite.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="row">
        <xs:annotation>
          <xs:documentation>Zahl: Die Zeile im Raster, in der das Objekt ausgegeben wird. Wird die Zeile nicht angegeben, so sucht sich das System selbständig eine passende freie Zeile für das Objekt. Maßangabe: die Position von der oberen Blattkante.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rotate">
        <xs:annotation>
          <xs:documentation>Dreht das Objekt. Ein Winkel &gt; 0 dreht das Objekt im Uhrzeigersinn, ein Winkel &lt; 0 gegen den Uhrzeigersinn. Wenn der Winkel ungleich 0 ist, wird die Rasterbelegung abgeschaltet.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rulewidth">
        <xs:annotation>
          <xs:documentation>Die Dicke des Rahmens, der um das Objekt gezogen wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="valign">
        <xs:annotation>
          <xs:documentation>Wird ein Objekt ausgegeben, das kein vielfaches der Rasterhöhe ist, dann gibt das Attribut die vertikale Ausrichtung an. Bottom beispielsweise bedeutet, dass der freie Platz oberhalb des Objekts bleibt. Voreinstellung ist “top”.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="top">
              <xs:annotation>
                <xs:documentation>Das Objekt wird oben ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="middle">
              <xs:annotation>
                <xs:documentation>Das Objekt wird so platziert, dass der Leerraum oberhalb und unterhalb derselbe ist.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bottom">
              <xs:annotation>
                <xs:documentation>Das Objekt wird unten ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="vreference">
        <xs:annotation>
          <xs:documentation>Bestimmt die Platzierung des Objekts relativ zur angegebenen Zeile.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="bottom">
              <xs:annotation>
                <xs:documentation>Die Zeile gibt die untere Kante des Objekts an.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="top">
              <xs:annotation>
                <xs:documentation>Die Zeile gibt die obere Kante des Objekts an (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Position">
    <xs:annotation>
      <xs:documentation>Ein Element zum aktuellen Overlay hinzufügen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="en:Image"/>
        <xs:element minOccurs="0" ref="en:Box"/>
        <xs:element minOccurs="0" ref="en:Circle"/>
        <xs:element minOccurs="0" ref="en:Rule"/>
        <xs:element minOccurs="0" ref="en:Textblock"/>
        <xs:element minOccurs="0" ref="en:Barcode"/>
        <xs:element minOccurs="0" ref="en:Table"/>
        <xs:element minOccurs="0" ref="en:Transformation"/>
        <xs:element minOccurs="0" ref="en:Frame"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
      </xs:sequence>
      <xs:attribute name="x" use="required">
        <xs:annotation>
          <xs:documentation>Die horizontale Position (0=links, 100=rechts).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="y" use="required">
        <xs:annotation>
          <xs:documentation>Die vertikale Position (0=unten, 100=oben).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="PositioningArea">
    <xs:annotation>
      <xs:documentation>Beschreibt einen Platzierungsbereich, der mehrere Platzierungsrahmen beinhalten kann. In diesen Rahmen können Elemente platziert werden</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:PositioningFrame"/>
        <xs:element ref="en:Loop"/>
      </xs:choice>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name des Platzierungsbereichs</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="framecolor">
        <xs:annotation>
          <xs:documentation>Setze die Farbe im Modus wenn die Platzierungsbereiche angezeigt werden (grid=yes). Voreinstellung ist 'red'.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="PositioningFrame">
    <xs:annotation>
      <xs:documentation>Beschreibt einen rechteckigen Bereich, in dem Elemente platziert werden können. Inhalte in diesen Bereichen werden bei Platzmangel automatisch in den nächsten Rahmen desselben Bereichs platziert.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="width" use="required">
        <xs:annotation>
          <xs:documentation>Breite des Rahmens in Rasterkästen</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="height" use="required">
        <xs:annotation>
          <xs:documentation>Höhe des Rahmens in Rasterkästen</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="row" use="required">
        <xs:annotation>
          <xs:documentation>Erste Zeile des Rahmens</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="column" use="required">
        <xs:annotation>
          <xs:documentation>Erste Spalte des Rahmens</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="ProcessNode">
    <xs:annotation>
      <xs:documentation>Bearbeitet alle angegebenen Datensätze. Die Elemente, für die die Regeln ausgeführt werden sollen, werden mit dem Attribut select angegeben.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="limit">
        <xs:annotation>
          <xs:documentation>Beschränkt die Anzahl der Datensätze, die mit diesem Befehl verarbeitet werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Auswahl der Kindelemente, die verarbeitet werden sollen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="mode">
        <xs:annotation>
          <xs:documentation>Name des Modus. Dieser muss bei Record exakt übereinstimmen. Damit ist es möglich für dasselbe Element unterschiedliche Regeln aufzurufen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Record">
    <xs:annotation>
      <xs:documentation>Enthält die Anweisungen, die bei dem angegebenen Datensatz ausgeführt werden sollen. Der oberste Datensatz (d.h. direkt unterhalb des Wurzelelements) wird vom Publisher aufgerufen, alle nachfolgenden Datensätze müssen über das Element BearbeiteKnoten aufgerufen werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:AddSearchpath"/>
        <xs:element minOccurs="0" ref="en:AttachFile"/>
        <xs:element minOccurs="0" ref="en:Bookmark"/>
        <xs:element minOccurs="0" ref="en:DefineColor"/>
        <xs:element minOccurs="0" ref="en:DefineFontfamily"/>
        <xs:element minOccurs="0" ref="en:DefineFontalias"/>
        <xs:element minOccurs="0" ref="en:DefineTextformat"/>
        <xs:element minOccurs="0" ref="en:ForAll"/>
        <xs:element minOccurs="0" ref="en:Group"/>
        <xs:element minOccurs="0" ref="en:Hyphenation"/>
        <xs:element minOccurs="0" ref="en:Include"/>
        <xs:element minOccurs="0" ref="en:InsertPages"/>
        <xs:element minOccurs="0" ref="en:Layout"/>
        <xs:element minOccurs="0" ref="en:LoadDataset"/>
        <xs:element minOccurs="0" ref="en:Loop"/>
        <xs:element minOccurs="0" ref="en:Message"/>
        <xs:element minOccurs="0" ref="en:NewPage"/>
        <xs:element minOccurs="0" ref="en:NextFrame"/>
        <xs:element minOccurs="0" ref="en:NextRow"/>
        <xs:element minOccurs="0" ref="en:Options"/>
        <xs:element minOccurs="0" ref="en:Output"/>
        <xs:element minOccurs="0" ref="en:Pagetype"/>
        <xs:element minOccurs="0" ref="en:Pageformat"/>
        <xs:element minOccurs="0" ref="en:PDFOptions"/>
        <xs:element minOccurs="0" ref="en:PlaceObject"/>
        <xs:element minOccurs="0" ref="en:ProcessNode"/>
        <xs:element minOccurs="0" ref="en:SaveDataset"/>
        <xs:element minOccurs="0" ref="en:SavePages"/>
        <xs:element minOccurs="0" ref="en:SetVariable"/>
        <xs:element minOccurs="0" ref="en:SortSequence"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
        <xs:element minOccurs="0" ref="en:Until"/>
        <xs:element minOccurs="0" ref="en:While"/>
      </xs:sequence>
      <xs:attribute name="element" use="required">
        <xs:annotation>
          <xs:documentation>Der Name des Elements der auf den Datensatz passt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="mode">
        <xs:annotation>
          <xs:documentation>Name des Modus der mit dem in ProcessNode übereinstimmt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Regular">
    <xs:annotation>
      <xs:documentation>Dient als Festlegung des Schriftschnittes, wenn keine Formatierung angegeben ist.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="fontface" use="required">
        <xs:annotation>
          <xs:documentation>Name der Schriftdatei, die geladen werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Rule">
    <xs:annotation>
      <xs:documentation>Zeichnet eine horizontale oder vertikale Linie im Raster.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="direction" use="required">
        <xs:annotation>
          <xs:documentation>Bestimmt die Ausrichtung der Linie.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="horizontal">
              <xs:annotation>
                <xs:documentation>Horizontale Linie</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="vertical">
              <xs:annotation>
                <xs:documentation>Vertikale Linie</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="rulewidth">
        <xs:annotation>
          <xs:documentation>Dicke der Linie in Rasterzellen oder als absolute Angabe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="length" use="required">
        <xs:annotation>
          <xs:documentation>Länge der Linie in Rasterzellen oder als absolute Angabe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="color">
        <xs:annotation>
          <xs:documentation>Farbe der Linie. Vorgabe: Schwarz</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="dashed">
        <xs:annotation>
          <xs:documentation>Benutze eine gestrichelte Linie.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeichne eine gestrichelte Linie.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Zeichne eine durchgezogene Linie.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="SaveDataset">
    <xs:annotation>
      <xs:documentation>Speichert eine XML-Datei auf Festplatte. Diese Datei muss als Kindelement hierarchische Struktur, zum Beispiel aus den Befehlen Element und Attribut enthalten, wahlweise zusammengesetzt durch Variablen. Eine ausführlichere Erläuterung findet sich im Abschnitt über automatisch generierte Verzeichnisse.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:Loop"/>
        <xs:element minOccurs="0" ref="en:Element"/>
        <xs:element minOccurs="0" ref="en:Makeindex"/>
        <xs:element minOccurs="0" ref="en:SortSequence"/>
        <xs:element minOccurs="0" ref="en:Copy-of"/>
        <xs:element minOccurs="0" ref="en:ForAll"/>
      </xs:sequence>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name der Dateisatzdatei. Beispiel: toc</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="elementname" use="required">
        <xs:annotation>
          <xs:documentation>Name des Wurzelelements, das erzeugt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="select">
        <xs:annotation>
          <xs:documentation>Alternativ zur Angabe des Datensatzes im Kindelement.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="attributes">
        <xs:annotation>
          <xs:documentation>Der Variablenname (als XPath-Ausdruck, also $foo), der Attribute-Elemente beinhaltet. Diese Attribute werden zum Wurzelelement hinzugefügt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="SavePages">
    <xs:annotation>
      <xs:documentation>Dieser Befehl ist für zwei unterschiedliche Szenarien gedacht:
      1: Alle Ausgaben innerhalb von SavePages werden intern gespeichert und nicht ins PDF ausgegeben. Sie können später mit InsertPages eingefügt werden. Hilfreich wenn die Ausgabe ggf. verworfen werden soll.
      2: Im Modus »Zukünftige Seiten« wurde schon Platz für diese Seiten mit InsertPages reserviert.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:AddSearchpath"/>
        <xs:element ref="en:AttachFile"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:DefineColor"/>
        <xs:element ref="en:DefineFontfamily"/>
        <xs:element ref="en:DefineFontalias"/>
        <xs:element ref="en:DefineTextformat"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Group"/>
        <xs:element ref="en:Hyphenation"/>
        <xs:element ref="en:Include"/>
        <xs:element ref="en:InsertPages"/>
        <xs:element ref="en:Layout"/>
        <xs:element ref="en:LoadDataset"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Message"/>
        <xs:element ref="en:NewPage"/>
        <xs:element ref="en:NextFrame"/>
        <xs:element ref="en:NextRow"/>
        <xs:element ref="en:Options"/>
        <xs:element ref="en:Output"/>
        <xs:element ref="en:Pagetype"/>
        <xs:element ref="en:Pageformat"/>
        <xs:element ref="en:PDFOptions"/>
        <xs:element ref="en:PlaceObject"/>
        <xs:element ref="en:ProcessNode"/>
        <xs:element ref="en:SaveDataset"/>
        <xs:element ref="en:SavePages"/>
        <xs:element ref="en:SetVariable"/>
        <xs:element ref="en:SortSequence"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Until"/>
        <xs:element ref="en:While"/>
      </xs:choice>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der Ausgabe, die verworfen (1) oder eingefügt (2) wird. Kann später oder früher mit InsertPages eingefügt werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="SetGrid">
    <xs:annotation>
      <xs:documentation>Setzt die Größe und Höhe eines Rasterkästchens. In dem Seitenraster können Elemente platziert werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Breite einer Rasterzelle. Entweder width oder nx benutzen, nicht beide.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="height">
        <xs:annotation>
          <xs:documentation>Höhe einer Rasterzelle. Entweder höhe oder ny angeben, nicht beide.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="nx">
        <xs:annotation>
          <xs:documentation>Anzahl der Rasterzellen in horizontaler Richtung. Entweder nx oder width angeben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ny">
        <xs:annotation>
          <xs:documentation>Gibt die Anzahl der Rasterzellen in vertikaler Richtung an. Entweder ny oder höhe angeben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="dx">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Rasterzellen (horizontal)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="dy">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Rasterzellen (horizontal)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="SetVariable">
    <xs:annotation>
      <xs:documentation>Weist einer Variablen einen Wert zu. Der Wert kann aus mehreren Elementen bestehen (siehe zweites Beispiel).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:Value"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Element"/>
        <xs:element ref="en:Attribute"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Td"/>
        <xs:element ref="en:Columns"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:TableNewPage"/>
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Tablehead"/>
        <xs:element ref="en:SortSequence"/>
      </xs:choice>
      <xs:attribute name="variable" use="required">
        <xs:annotation>
          <xs:documentation>Name der Variablen, der etwas zugewiesen werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="select">
        <xs:annotation>
          <xs:documentation>Inhalt, der der Variablen zugewiesen wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="trace">
        <xs:annotation>
          <xs:documentation>Zeige Informationen über die Zuweisung in der Logdatei.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeige Informationen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Zeige keine Informationen (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="SortSequence">
    <xs:annotation>
      <xs:documentation>Sortiert eine Liste.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Datensatz, der sortiert werden soll</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="removeduplicates">
        <xs:annotation>
          <xs:documentation>Wenn Duplikate gelöscht werden sollen, steht hier das Attribut mit dem Inhalt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="criterion" use="required">
        <xs:annotation>
          <xs:documentation>Attributname, nach dem sortiert werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="numerical">
        <xs:annotation>
          <xs:documentation>Sortiere alphabetisch oder numerisch</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Sortiere numerisch</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Sortiere alphabetisch (Voreinstellung)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="order">
        <xs:annotation>
          <xs:documentation>Wähle die Sortierreihenfolge</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="ascending">
              <xs:annotation>
                <xs:documentation>Benutze aufsteigende Sortierreihenfolge (Voreinstellung)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="descending">
              <xs:annotation>
                <xs:documentation>Benutze absteigende Sortierreihenfolge</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Span">
    <xs:annotation>
      <xs:documentation>Text mit Style-Optionen umgeben.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:A"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Color"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:NoBreak"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:URL"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>CSS Klasse für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-color">
        <xs:annotation>
          <xs:documentation>Die Hintergrundfarbe des Inhalts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-padding-top">
        <xs:annotation>
          <xs:documentation>Der Innenabstand (oben) der Hintergrundfarbe (kann negativ sein)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-padding-bottom">
        <xs:annotation>
          <xs:documentation>Der Innenabstand (unten) der Hintergrundfarbe (kann negativ sein)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="letter-spacing">
        <xs:annotation>
          <xs:documentation>Erhöhe Leerraum zwischen Zeichen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Sub">
    <xs:annotation>
      <xs:documentation>Erzeugt tiefgestellten Text.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="en:Value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Sup">
    <xs:annotation>
      <xs:documentation>Erzeugt hochgestellten Text.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="en:Value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Switch">
    <xs:annotation>
      <xs:documentation>Erzeugt eine „Wenn-Dann-Abfrage“. Das heißt, für jedes Kindelement Case wird überprüft, ob die Bedingung erfüllt ist. Wenn ja, dann wird der Inhalt des Kindelements Case ausgeführt und die Verarbeitung hinter der Fallunterscheidung fortgesetzt. Wenn keine Bedingung erfüllt wurde und ein Element Otherwise gefunden wurde, wird der         Inhalt des Elements Otherwise ausgeführt. Durch das frühzeitige Abbrechen werden nachfolgende Fälle nicht weiter untersucht (siehe Beispiel).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="en:Case"/>
        <xs:element minOccurs="0" ref="en:Otherwise"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Stylesheet">
    <xs:annotation>
      <xs:documentation>Lädt eine CSS-Datei oder definiert CSS-Eigenschaften</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="filename">
        <xs:annotation>
          <xs:documentation>Der Dateiname der CSS Datei (inklusive Dateiendung).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Table">
    <xs:annotation>
      <xs:documentation>Erzeugt eine Tabelle, die in etwa dem HTML Tabellenmodell entspricht.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Tablehead"/>
        <xs:element ref="en:Tablefoot"/>
        <xs:element ref="en:TableNewPage"/>
        <xs:element ref="en:Columns"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Message"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:ForAll"/>
      </xs:choice>
      <xs:attribute name="balance">
        <xs:annotation>
          <xs:documentation>Versucht die letzte Seite anhand der Anzahl der Rahmen auszubalancieren. Experimentell</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Versucht die Spalten auszugleichen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Erste Spalte wird zuerst ausgefüllt</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="border-collapse">
        <xs:annotation>
          <xs:documentation>Bestimmt, ob benachbarte Zellen die Rahmen teilen. Das Verhalten von border-collapse="collapse" ist nicht definiert wenn die Tablle einen Zeilen- oder Spaltenabstand &gt; 0 haben oder wenn die benachbarten Zellen nicht dieselbe Strichstärke und -farbe haben.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="separate">
              <xs:annotation>
                <xs:documentation>Die Rahmen sind Teil der Zelle und werden nicht mit den Nachbarzellen geteilt.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="collapse">
              <xs:annotation>
                <xs:documentation>Die Rahmen benachbarter Zellen überlappen sich.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="columndistance">
        <xs:annotation>
          <xs:documentation>Der Abstand zwischen zwei Spalten.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="eval">
        <xs:annotation>
          <xs:documentation>Wertet den angegebenen XPath-Ausdruck aus und ignoriert dessen Ausgabe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fontface">
        <xs:annotation>
          <xs:documentation>Name der Schriftfamilie, die benutzt werden soll. Wenn keine Schriftart angegeben ist, wird auf die Schriftart ›text‹ umgeschaltet.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leading">
        <xs:annotation>
          <xs:documentation>Der Abstand zwischen zwei Zeilen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding">
        <xs:annotation>
          <xs:documentation>Der Innenabstand innerhalb einer Tabellenzelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="stretch">
        <xs:annotation>
          <xs:documentation>Wenn der Tabelleninhalt schmal ist und stretch den Wert nein hat, dann nimmt die Tabelle nur den benötigten Platz ein. Wenn der Tabelleninhalt breit ist oder stretch den Wert max hat, dann nimmt die Tabelle den Wert aus dem Attribut breite ein.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="max">
              <xs:annotation>
                <xs:documentation>Die Tabelle zur angegebenen Breite dehnen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Die Tabelle nimmt den kleinstmöglichen Platz ein.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="textformat">
        <xs:annotation>
          <xs:documentation>Textformat für die Tabelle. Voreinstellung ist __leftaligned, wenn keine Ausrichtung (align) angegeben wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Der Bereich, den die Tabelle (maximal) im Raster belegt. Angabe in Rasterzellen oder absoluten Längenangaben. Voreinstellung ist der verfügbare Platz.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tablefoot">
    <xs:annotation>
      <xs:documentation>Gibt den Tabellenfuß an.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:ForAll"/>
      </xs:choice>
      <xs:attribute name="page">
        <xs:annotation>
          <xs:documentation>Der Tabellenfuß wird nur auf den angegebenen Seiten platziert. Voreinstellung ist, dass er auf jeder Seite erscheint.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="all">
              <xs:annotation>
                <xs:documentation>Tabellenfuß auf allen Seiten darstellen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="last">
              <xs:annotation>
                <xs:documentation>Den Tabellenfuß nur auf der letzten Seite anzeigen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tablehead">
    <xs:annotation>
      <xs:documentation>Gibt den Tabellenkopf an.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Switch"/>
      </xs:choice>
      <xs:attribute name="page">
        <xs:annotation>
          <xs:documentation>Die Seite, auf der der Tabellenkopf erscheinen soll. Voreinstellung ist »alle«</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="first">
              <xs:annotation>
                <xs:documentation>Nur auf der ersten Seite.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="all">
              <xs:annotation>
                <xs:documentation>Auf allen Seiten. Falls »first« definiert, erscheint der Tabellenkopf auf allen Seiten bis auf der ersten.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="TableNewPage">
    <xs:annotation>
      <xs:documentation>Wechselt auf eine neue Seite innerhalb einer Tabelle.</xs:documentation>
    </xs:annotation>
    <xs:complexType/>
  </xs:element>
  <xs:element name="Td">
    <xs:annotation>
      <xs:documentation>Erstellt eine Tabellenzelle, wie in HTML</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Barcode"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:Box"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:Groupcontents"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Overlay"/>
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:VSpace"/>
      </xs:choice>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>Die CSS-Klasse für die Formtierung.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für diese Tabellenzelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="align">
        <xs:annotation>
          <xs:documentation>Die horizontale Ausrichtung des Tabelleninhalts</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="left">
                  <xs:annotation>
                    <xs:documentation>Der Inhalt der Zelle ist am linken Rand ausgerichtet. (Voreinstellung)</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="right">
                  <xs:annotation>
                    <xs:documentation>Der Inhalt der Zelle ist rechtsbündig ausgerichtet.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="center">
                  <xs:annotation>
                    <xs:documentation>Der Inhalt der Zelle ist auf der Mittelachse ausgerichtet mit links und rechts Flatterrand.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="justify">
                  <xs:annotation>
                    <xs:documentation>Text im Blocksatz.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Der Name der Hintergrundfarbe, wenn die Zelle eingefärbt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-font-family">
        <xs:annotation>
          <xs:documentation>Setzt die Fontfamilie für den Hintergrundtext. Voreinstellung ist die Fontfamilie für die Tabelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-textcolor">
        <xs:annotation>
          <xs:documentation>Die Farbe des Hintergrundtexts, falls vorhanden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-size">
        <xs:annotation>
          <xs:documentation>Bestimmt die Größe des Hintergrundtexts. Derzeit ist nur 'contain' und 'auto' erlaubt.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="contain">
              <xs:annotation>
                <xs:documentation>Der Text passt sich der Zellenhöhe an.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="auto">
              <xs:annotation>
                <xs:documentation>Der Hintergrundtext wird nicht skaliert.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="background-transform">
        <xs:annotation>
          <xs:documentation>Die Transformation des Hintergrundtexts (falls vorhanden). Derzeit wird folgende Angabe unterstützt: rotate(-40deg) (und andere Winkel im Bereich 0 bis -90).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="background-text">
        <xs:annotation>
          <xs:documentation>Ein Text, der in den Tabellenhintergrund gelegt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-left">
        <xs:annotation>
          <xs:documentation>Die Breite der Linie links. Die Linie liegt innerhalb der Zelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-right">
        <xs:annotation>
          <xs:documentation>Die Breite der Linie rechts. Die Linie liegt innerhalb der Zelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top">
        <xs:annotation>
          <xs:documentation>Die Breite der Linie oben. Die Linie liegt innerhalb der Zelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom">
        <xs:annotation>
          <xs:documentation>Die Breite der Linie unten. Die Linie liegt innerhalb der Zelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-left-color">
        <xs:annotation>
          <xs:documentation>Die Farbe der Linie links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-right-color">
        <xs:annotation>
          <xs:documentation>Die Farbe der Linie rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top-color">
        <xs:annotation>
          <xs:documentation>Die Farbe der Linie oben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom-color">
        <xs:annotation>
          <xs:documentation>Die Farbe der Linie unten.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="colspan">
        <xs:annotation>
          <xs:documentation>Die Anzahl der Spalten, die die Zelle überdecken soll. Voreinstellung ist 1.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding">
        <xs:annotation>
          <xs:documentation>Kurzform für die Bestimmung von padding-top und den anderen drei Werten mit derselben Maßangabe</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-top">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand einer Tabelle (oben)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-bottom">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand einer Tabelle (unten)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-left">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand einer Tabelle (links)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-right">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand einer Tabelle (rechts)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rowspan">
        <xs:annotation>
          <xs:documentation>Die Anzahl der Zeilen, die die Zelle überdecken soll. Voreinstellung ist 1.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rotate">
        <xs:annotation>
          <xs:documentation>Drehe den Inhalt (im Uhrzeigersinn) der Tabellenzelle. Experimentell und derzeit nur für Textinhalte.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="valign">
        <xs:annotation>
          <xs:documentation>Die vertikale Ausrichtung des Zelleninhalts.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="top">
                  <xs:annotation>
                    <xs:documentation>Der Zellinhalt ist an der oberen Kante ausgerichtet.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="middle">
                  <xs:annotation>
                    <xs:documentation>Der Zellinhalt vertikal zentriert.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="bottom">
                  <xs:annotation>
                    <xs:documentation>Der Zellinhalt ist an der unteren Kante ausgerichtet.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tablerule">
    <xs:annotation>
      <xs:documentation>Erzeugt eine horizontale Linie in einer Tabelle</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="rulewidth">
        <xs:annotation>
          <xs:documentation>Die »Dicke« der Linie. Voreinstellung ist 0.25 Punkt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="color">
        <xs:annotation>
          <xs:documentation>Farbe der Linie. (Default: Schwarz)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="start">
        <xs:annotation>
          <xs:documentation>Die erste Spalte der Linie. Voreinstellung ist 1.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="break-below">
        <xs:annotation>
          <xs:documentation>Erlaube einen Umbruch unterhalb der Linie?</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes"/>
            <xs:enumeration value="no"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>CSS Klasse für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Textblock">
    <xs:annotation>
      <xs:documentation>Erzeugt einen rechteckigen Bereich mit Text.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:Ol"/>
        <xs:element ref="en:Ul"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:Value"/>
        <xs:element ref="en:ForAll"/>
      </xs:choice>
      <xs:attribute name="angle">
        <xs:annotation>
          <xs:documentation>Winkel (Drehrichtung gegen den Uhrzeigersinn), um der der Text gedreht wird. Die Spalte und die Zeile, in der der Text ausgegeben wird, bleibt gleich, die Breite und die Höhe des Textes ändert sich gegenüber dem ursprünglichen Text.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="color">
        <xs:annotation>
          <xs:documentation>Name der Farbe im Textblock.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="columns">
        <xs:annotation>
          <xs:documentation>Anzahl der Spalten in dem Textblock. Mehrere Spalten nur für normalen Text benutzen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="columndistance">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Spalten. Voreinstellung: 3mm.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fontface">
        <xs:annotation>
          <xs:documentation>Name der Schriftfamilie, die benutzt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="minheight">
        <xs:annotation>
          <xs:documentation>Die minimale Höhe des Textblocks als Längenangabe oder als Maßeinheit.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="textformat">
        <xs:annotation>
          <xs:documentation>Name des zu benutzenden Textformats. Wird kein Textformat angegeben, nimmt das System das Textformat text.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Anzahl der Spalten, die der Text einnehmen soll. Wenn das Attribut nicht angegeben wird, so bestimmt das umliegende Element die Breite.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="e_Text">
    <xs:sequence>
      <xs:element ref="en:Text"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Text">
    <xs:annotation>
      <xs:documentation>Erzeugt einen Text, der über die Grenzen von Positionierungsrahmen umbrechen kann. Wird mit Output benutzt.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="en:Action"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Ol"/>
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Ul"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
      <xs:attribute name="color">
        <xs:annotation>
          <xs:documentation>Name der Farbe im Textblock.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fontface">
        <xs:annotation>
          <xs:documentation>Name der Schriftfamilie, die benutzt werden soll. Voreinstellung ist text</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="textformat">
        <xs:annotation>
          <xs:documentation>Name des zu benutzenden Textformats. Wird kein Textformat angegeben, nimmt das System das Textformat text.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Trace">
    <xs:annotation>
      <xs:documentation>Setze Debugging-Schalter</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="assignments">
        <xs:annotation>
          <xs:documentation>Schreibt Zusweisungen (SetVariable) in die Logdatei.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zusätzliche Ausgaben in der Logdatei.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Normaler Lauf (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="grid">
        <xs:annotation>
          <xs:documentation>Wenn 'yes', dann zeichnet der Publisher das zugrunde liegende Raster auf die Seiten.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeige das Raster.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Das Raster wird nicht angezeigt (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="gridallocation">
        <xs:annotation>
          <xs:documentation>Wenn 'yes', dann markiert der Publisher die belegten Rasterzellen mit einer Hintergrundfarbe.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeige Rasterbelegung.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Rasterbelegung nicht zeigen (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="hyphenation">
        <xs:annotation>
          <xs:documentation>Bei 'yes' markiert der speedata Publisher die möglichen Trennstellen für die Silbentrennung.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeige Silbentrennungsmöglichkeiten.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Keine Silbentrennungsmöglichkeiten zeigen (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="objects">
        <xs:annotation>
          <xs:documentation>Zeichnet Rechtecke um Objekte.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeichnet Rechtecke</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Keine Rechtecke um Objekte (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="verbose">
        <xs:annotation>
          <xs:documentation>Erzeugt zusätzliche Ausgaben in der Protokolldatei.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zusätzliche Ausgaben in der Logdatei.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Normaler Lauf (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="textformat">
        <xs:annotation>
          <xs:documentation>Zeige Textformat als Tooltip im PDF</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Zeige Textformat</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Zeige Textformat nicht (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tr">
    <xs:annotation>
      <xs:documentation>Erstellt eine Tabellenzeile</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:Message"/>
        <xs:element minOccurs="0" ref="en:Td"/>
        <xs:element minOccurs="0" ref="en:Copy-of"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
        <xs:element minOccurs="0" ref="en:ForAll"/>
        <xs:element minOccurs="0" ref="en:Loop"/>
      </xs:sequence>
      <xs:attribute name="align">
        <xs:annotation>
          <xs:documentation>Die horizontale Ausrichtung des Tabelleninhalts (gilt für die gesamte Zeile).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="left">
              <xs:annotation>
                <xs:documentation>Der Inhalt der Zelle ist am linken Rand ausgerichtet. (Voreinstellung)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="right">
              <xs:annotation>
                <xs:documentation>Der Inhalt der Zelle ist rechtsbündig ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="center">
              <xs:annotation>
                <xs:documentation>Der Inhalt der Zelle ist auf der Mittelachse ausgerichtet mit links und rechts Flatterrand.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="justify">
              <xs:annotation>
                <xs:documentation>Text im Blocksatz.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Die Farbe der Zellen in der Zeile.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="break-below">
        <xs:annotation>
          <xs:documentation>Erlaubt einen Tabellenumbruch zwischen dieser und der nächsten Zeile.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="yes">
                  <xs:annotation>
                    <xs:documentation>Erlaubt einen Tabellenumbruch zwischen dieser und der nächsten Zeile (Voreinstellung).</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="no">
                  <xs:annotation>
                    <xs:documentation>Verhindert einen Tabellenumbruch zwischen dieser und der nächsten Zeile.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="data">
        <xs:annotation>
          <xs:documentation>Auf diese Daten können im Tabellenkopf und Tabellenfuß mit $_last_tr_data zugegriffen werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="minheight">
        <xs:annotation>
          <xs:documentation>Die Mindesthöhe der Zeile in Rasterzellen oder als Längenangabe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="sethead">
        <xs:annotation>
          <xs:documentation>Diese Zeile für zukünftige Kopfzeilen benutzen.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="yes">
                  <xs:annotation>
                    <xs:documentation>Diese Zeile für zukünftige Kopfzeilen benutzen.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="no">
                  <xs:annotation>
                    <xs:documentation>Diese Zeile wird nicht besonders behandelt (Voreinstellung).</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="clear">
                  <xs:annotation>
                    <xs:documentation>Kopf löschen. Weitere Seiten haben keine Kopfzeile bis eine neue gesetzt wird.</xs:documentation>
                  </xs:annotation>
                </xs:enumeration>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:pattern value="\{.+\}"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="top-distance">
        <xs:annotation>
