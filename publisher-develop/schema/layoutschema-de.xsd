        <xs:element minOccurs="0" ref="en:SaveDataset"/>
        <xs:element minOccurs="0" ref="en:SavePages"/>
        <xs:element minOccurs="0" ref="en:SetGrid"/>
        <xs:element minOccurs="0" ref="en:SetVariable"/>
        <xs:element minOccurs="0" ref="en:SortSequence"/>
        <xs:element minOccurs="0" ref="en:Span"/>
        <xs:element minOccurs="0" ref="en:Sub"/>
        <xs:element minOccurs="0" ref="en:Sup"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
        <xs:element minOccurs="0" ref="en:Table"/>
        <xs:element minOccurs="0" ref="en:TableNewPage"/>
        <xs:element minOccurs="0" ref="en:Tablehead"/>
        <xs:element minOccurs="0" ref="en:Tablerule"/>
        <xs:element minOccurs="0" ref="en:Td"/>
        <xs:element minOccurs="0" ref="en:Tr"/>
        <xs:element minOccurs="0" ref="en:Trace"/>
        <xs:element minOccurs="0" ref="en:U"/>
        <xs:element minOccurs="0" ref="en:Until"/>
        <xs:element minOccurs="0" ref="en:Value"/>
        <xs:element minOccurs="0" ref="en:While"/>
      </xs:sequence>
      <xs:attribute name="test" use="required">
        <xs:annotation>
          <xs:documentation>Für jeden Fall in einer Fallunterscheidung wird überprüft, ob die Bedingung erfüllt ist und bei Übereinstimmung die Kindelemente ausgeführt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Circle">
    <xs:annotation>
      <xs:documentation>Erzeugt einen Kreis oder eine Ellipse.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Farbe des Kreises.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>CSS Klasse für diesen Kreis.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="framecolor">
        <xs:annotation>
          <xs:documentation>Farbe der Kreislinie.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für diesen Kreis.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="radiusx" use="required">
        <xs:annotation>
          <xs:documentation>Radius des Kreises in Rasterzellen (horizontal) oder als absolute Maßangabe. In Verbindung mit radiusy lässt sich eine Ellipse erzeugen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="radiusy">
        <xs:annotation>
          <xs:documentation>Radius der Ellipse in Rasterzellen (vertikal) oder als absolute Maßangabe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rulewidth">
        <xs:annotation>
          <xs:documentation>Die Dicke des Striches um den Kreis.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Color">
    <xs:annotation>
      <xs:documentation>Auf eine andere Textfarbe wechseln</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:A"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Color"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:NoBreak"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:URL"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der Farbe. Muss mit DefineColor vorher definiert werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Column">
    <xs:annotation>
      <xs:documentation>Gibt die Eigenschaften einer Spalte in einer Tabelle an.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="align">
        <xs:annotation>
          <xs:documentation>Die horizontale Ausrichtung des Tabelleninhalts. Kann in einer Zelle (Td) überschrieben werden.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="left">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden linksbündig ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="center">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden horizontal zentriert.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="right">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden rechtsbündig ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Farbe der Tabellenspalte.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-left">
        <xs:annotation>
          <xs:documentation>Setze das linke padding für diese Spalte</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-right">
        <xs:annotation>
          <xs:documentation>Setze das rechte padding für diese Spalte</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Breite der Spalte. Kann entweder in Rasterzellen (Zahl), absoluten Maßangaben (z.B. 1cm) oder in *-Angaben (z.B. 4*) angegeben werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="valign">
        <xs:annotation>
          <xs:documentation>Die vertikale Ausrichtung des Tabelleninhalts. Kann in einer Zelle (Td) überschrieben werden.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="top">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden oben ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="middle">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden vertikal zentriert ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bottom">
              <xs:annotation>
                <xs:documentation>Die Tabellenzellen dieser Spalte werden  unten ausgerichtet.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Columns">
    <xs:annotation>
      <xs:documentation>Legt die Spaltenbreiten und andere Eigenschaften einer Tabelle fest.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Column"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Compatibility">
    <xs:annotation>
      <xs:documentation>Setze Kompatibilität für ältere Layoutregeln</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="movecursoronplaceobject">
        <xs:annotation>
          <xs:documentation>Wechselt das Verhalten bei dem Objekte (platziert mit PlaceObject) am rechten Rand den Cursor in Spalte »Anzahl Spalten + 1« setzt.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Neues Verhalten (Voreinstellung): setzt den Cursor in Spalte 1 wenn ein Objekt an den rechten Rand stößt.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Altes Verhalten: setzt den Cursor auf die Spalte 1 hinter den rechten Rand des platzierten Objekts.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Contents">
    <xs:annotation>
      <xs:documentation>Kennzeichnet den Inhalt einer Group, einer virtuellen Fläche.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:AddSearchpath"/>
        <xs:element minOccurs="0" ref="en:AttachFile"/>
        <xs:element minOccurs="0" ref="en:Bookmark"/>
        <xs:element minOccurs="0" ref="en:DefineColor"/>
        <xs:element minOccurs="0" ref="en:DefineFontfamily"/>
        <xs:element minOccurs="0" ref="en:DefineFontalias"/>
        <xs:element minOccurs="0" ref="en:DefineTextformat"/>
        <xs:element minOccurs="0" ref="en:ForAll"/>
        <xs:element minOccurs="0" ref="en:Group"/>
        <xs:element minOccurs="0" ref="en:Hyphenation"/>
        <xs:element minOccurs="0" ref="en:Include"/>
        <xs:element minOccurs="0" ref="en:InsertPages"/>
        <xs:element minOccurs="0" ref="en:Layout"/>
        <xs:element minOccurs="0" ref="en:LoadDataset"/>
        <xs:element minOccurs="0" ref="en:Loop"/>
        <xs:element minOccurs="0" ref="en:Message"/>
        <xs:element minOccurs="0" ref="en:NewPage"/>
        <xs:element minOccurs="0" ref="en:NextFrame"/>
        <xs:element minOccurs="0" ref="en:NextRow"/>
        <xs:element minOccurs="0" ref="en:Options"/>
        <xs:element minOccurs="0" ref="en:Output"/>
        <xs:element minOccurs="0" ref="en:Pagetype"/>
        <xs:element minOccurs="0" ref="en:Pageformat"/>
        <xs:element minOccurs="0" ref="en:PDFOptions"/>
        <xs:element minOccurs="0" ref="en:PlaceObject"/>
        <xs:element minOccurs="0" ref="en:ProcessNode"/>
        <xs:element minOccurs="0" ref="en:SaveDataset"/>
        <xs:element minOccurs="0" ref="en:SavePages"/>
        <xs:element minOccurs="0" ref="en:SetVariable"/>
        <xs:element minOccurs="0" ref="en:SortSequence"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
        <xs:element minOccurs="0" ref="en:Until"/>
        <xs:element minOccurs="0" ref="en:While"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Copy-of">
    <xs:annotation>
      <xs:documentation>Ersetzt dieses Element mit dem Inhalt von select als Elementstruktur. Dient zum „Zusammenbauen“ von Texten und Tabellen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Die Auswahl (eine Variable) die kopiert werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefineColorprofile">
    <xs:annotation>
      <xs:documentation>Weist einem externen Farbprofil einem Namen zu. Für PDFOptions.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="condition">
        <xs:annotation>
          <xs:documentation>Beschreibung der Ausgabebedingung.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="colors" use="required">
        <xs:annotation>
          <xs:documentation>Anzahl der Farben in dem Farbprofil</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="filename" use="required">
        <xs:annotation>
          <xs:documentation>Dateiname des Farbprofils.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="identifier" use="required">
        <xs:annotation>
          <xs:documentation>Der offizielle Name in der Registrierung.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="info" use="required">
        <xs:annotation>
          <xs:documentation>Zusammenfassung des Profils.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der interne Name des Farbprofils. Für PDFOptions.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="registry">
        <xs:annotation>
          <xs:documentation>Der Name der Registrierung. Voreinstellung ist http://www.color.org/.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefineColor">
    <xs:annotation>
      <xs:documentation>Definiert Farben, die später im Layout benutzt werden können</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="colorname">
        <xs:annotation>
          <xs:documentation>Der Name der Sonderfarbe wenn das Modell auf »spotcolor« eingestellt ist. Der muss den offiziellen Namen tragen, wie z.B. »PANTONE 116 C«.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="model">
        <xs:annotation>
          <xs:documentation>Das Farbmodell, das benutzt werden soll. Derzeit werden »rgb«, »cmyk«, “gray” und spotcolor« unterstützt.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="cmyk">
              <xs:annotation>
                <xs:documentation>CMYK (Türkis, Magenta, Gelb und Schwarz), Werte zwischen 0 und 100 (100 = volle Intensität)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="rgb">
              <xs:annotation>
                <xs:documentation>RGB (Rot, Grün, Blau), Werte zwischen 0 und 100, 100 bedeutet volle Intensität</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RGB">
              <xs:annotation>
                <xs:documentation>RGB (Rot, Grün, Blau), Werte zwischen 0 und 255, 255 bedeutet volle Intensität</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="gray">
              <xs:annotation>
                <xs:documentation>Graustufen (0=Schwarz,100=Weiß)</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="spotcolor">
              <xs:annotation>
                <xs:documentation>Eine PANTONE oder HKS Farbe.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name der Farbe, die definiert wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="overprint">
        <xs:annotation>
          <xs:documentation>Überdrucken für diese Farbe anschalten</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Überdrucken für diese Farbe anschalten.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Überdrucken für diese Farbe ausschalten (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="value">
        <xs:annotation>
          <xs:documentation>Der Hexwert der Farbe, wie beispielsweise #FA5 oder #FFAA55.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="r">
        <xs:annotation>
          <xs:documentation>Rot Anteil bei RGB</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="g">
        <xs:annotation>
          <xs:documentation>Grün Anteil bei RGB</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="b">
        <xs:annotation>
          <xs:documentation>Blau Anteil bei RGB</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="c">
        <xs:annotation>
          <xs:documentation>Cyan Anteil bei CMYK</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="m">
        <xs:annotation>
          <xs:documentation>Magenta Anteil bei CMYK</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="y">
        <xs:annotation>
          <xs:documentation>Gelb Anteil bei CMYK</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="k">
        <xs:annotation>
          <xs:documentation>Schwarz (key) Anteil bei CMYK</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefineFontalias">
    <xs:annotation>
      <xs:documentation>Definiert einen Alias für die Verwendung bei DefineFontfamily. Aliase werden rekursiv aufgelöst.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="alias" use="required">
        <xs:annotation>
          <xs:documentation>Neuer (gleichberechtigter) Name der Schriftart</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="existing" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der existierenden Schriftfamilie.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefineFontfamily">
    <xs:annotation>
      <xs:documentation>Das Element DefineFontfamily definiert eine Schriftfamilie, auf in den Elementen Paragraph, Textblock, Fontface und Table mit dem Attribut fontface zugegriffen werden kann.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Regular"/>
        <xs:element ref="en:Italic"/>
        <xs:element ref="en:Bold"/>
        <xs:element ref="en:BoldItalic"/>
      </xs:choice>
      <xs:attribute name="fontsize" use="required">
        <xs:annotation>
          <xs:documentation>Schriftgröße in pt (PostScript-Punkte, DTP-Punkte). Zahl ohne Einheit.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leading" use="required">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Grundlinien in pt (PostScript-Punkte).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Interner Name, unter dem die Schriftfamilie später angesprochen wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="DefineTextformat">
    <xs:annotation>
      <xs:documentation>Definiert ein Textformat. Ein Textformat dient zur Formatierung von Absätzen (Einrückung und Ausrichtung). Auf die definierten Textformate kann im Layout (in den Elementen Paragraph und Textblock) zurückgegriffen werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="alignment">
        <xs:annotation>
          <xs:documentation>Bestimmt die Formatierung des Textes.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="justified">
              <xs:annotation>
                <xs:documentation>Der Text hat eine rechteckige Form.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="leftaligned">
              <xs:annotation>
                <xs:documentation>Der Text flattert am rechten Rand.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="rightaligned">
              <xs:annotation>
                <xs:documentation>Der Text flattert am linken Rand</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="centered">
              <xs:annotation>
                <xs:documentation>Der Text ist im Flattersatz an beiden Rändern (rechts und links) gesetzt.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="border-top">
        <xs:annotation>
          <xs:documentation>Die Dicke der Linie über dem Text</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom">
        <xs:annotation>
          <xs:documentation>Die Dicke der Linie unter dem Text</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="break-below">
        <xs:annotation>
          <xs:documentation>Umbruch unterhalb des Textes verbieten/erlauben. Das funktioniert nur, wenn der nachfolgende Text innerhalb derselben PlaceObject oder Output Umgebung enthalten ist.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Umbruch unterhalb des Textes erlauben (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Seitenumbruch unterhalb des Textes verhindern.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="column-padding-top">
        <xs:annotation>
          <xs:documentation>Die Höhe des Innenabstands in einer Spalte (oben) mit Output/Text</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fill-last-line">
        <xs:annotation>
          <xs:documentation>Mindestlänge der letzten Zeile im Absatz. Prozentangabe (0 bis 100). Mit Vorsicht anzuwenden. Voreinstellung ist 0.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="hyphenchar">
        <xs:annotation>
          <xs:documentation>Das Zeichen, das für Trennung benutzt wird (Voreinstellung: -)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="hyphenate">
        <xs:annotation>
          <xs:documentation>Silbentrennung erlauben oder abschalten (Voreinstellung ist an).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Silbentrennung erlauben (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Silbentrennung für den Absatz verhindern.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="indentation">
        <xs:annotation>
          <xs:documentation>Die Länge des Absatzeinzugs.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="margin-bottom">
        <xs:annotation>
          <xs:documentation>Abstand zwischen der Linie unten und dem nachfolgenden Absatz.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="margin-top">
        <xs:annotation>
          <xs:documentation>Abstand zwischen der Linie oben und dem vorhergehenden Text.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name des Textformats, unter dem es später im Layoutregelwerk angesprochen wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="orphan">
        <xs:annotation>
          <xs:documentation>Wenn »yes«, Schusterjungen erlauben (erste Zeile eines Absatzes ist auf der vorherigen Seite). Wenn eine Zahl angegeben wird, ist dies die Anzahl der Zeilen, die zusammengehalten werden. Voreinstellung ist »no«.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]+|yes|no"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="padding-top">
        <xs:annotation>
          <xs:documentation>Abstand zwischen der Oberkante des Texts und der Linie oben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rows">
        <xs:annotation>
          <xs:documentation>Anzahl der Zeilen, die eingezogen werden (wenn indentation angegeben ist) bzw. falls die Zahl negativ ist, die Anzahl der Zeilen, die nicht eingerückt werden.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="tab">
        <xs:annotation>
          <xs:documentation>Was ist beim Tabulator (&amp; #09;) zu tun?</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="space">
              <xs:annotation>
                <xs:documentation>Tabulator als Leerzeichen interpretieren</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="hspace">
              <xs:annotation>
                <xs:documentation>Interpretiere Tabulator als dehnbaren Leerraum</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="widow">
        <xs:annotation>
          <xs:documentation>Wenn »yes«, Hurenkinder erlauben (letzte Zeile eines Absatzes ist auf der nächsten Seite). Wenn eine Zahl angegeben wird, ist dies die Anzahl der Zeilen, die zusammengehalten werden. Voreinstellung ist »no«.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]+|yes|no"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Element">
    <xs:annotation>
      <xs:documentation>Erzeugt eine Datenstruktur, die mithilfe von SaveDataset auf Festplatte gespeichert werden kann.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Attribute"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:Element"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Makeindex"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:SortSequence"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Name des Elements, das erzeugt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Fallback">
    <xs:annotation>
      <xs:documentation>Definiere ein Fallback für LoadFontfile</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="filename" use="required">
        <xs:annotation>
          <xs:documentation>Der Dateiname der Schriftart, die als Fallback benutzt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Fontface">
    <xs:annotation>
      <xs:documentation>Schaltet vorübergehend auf eine andere Schriftfamilie um.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:A"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Color"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:NoBreak"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:URL"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
      <xs:attribute name="fontfamily" use="required">
        <xs:annotation>
          <xs:documentation>Schaltet auf die angegebene Schriftfamilie um.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="ForAll">
    <xs:annotation>
      <xs:documentation>Ruft die Kindelemente für alle Daten-Elemente auf, die auf das Muster bei select passen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Action"/>
        <xs:element ref="en:AddSearchpath"/>
        <xs:element ref="en:Attribute"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Barcode"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Column"/>
        <xs:element ref="en:Columns"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:DefineColor"/>
        <xs:element ref="en:DefineFontfamily"/>
        <xs:element ref="en:DefineFontalias"/>
        <xs:element ref="en:DefineTextformat"/>
        <xs:element ref="en:Element"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:Group"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:Hyphenation"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Initial"/>
        <xs:element ref="en:Include"/>
        <xs:element ref="en:InsertPages"/>
        <xs:element ref="en:Layout"/>
        <xs:element ref="en:Li"/>
        <xs:element ref="en:LoadDataset"/>
        <xs:element ref="en:LoadFontfile"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Message"/>
        <xs:element ref="en:NewPage"/>
        <xs:element ref="en:NextFrame"/>
        <xs:element ref="en:NextRow"/>
        <xs:element ref="en:Options"/>
        <xs:element ref="en:Output"/>
        <xs:element ref="en:Pageformat"/>
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:PlaceObject"/>
        <xs:element ref="en:PositioningArea"/>
        <xs:element ref="en:PositioningFrame"/>
        <xs:element ref="en:ProcessNode"/>
        <xs:element ref="en:SaveDataset"/>
        <xs:element ref="en:SavePages"/>
        <xs:element ref="en:SetGrid"/>
        <xs:element ref="en:SetVariable"/>
        <xs:element ref="en:SortSequence"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:TableNewPage"/>
        <xs:element ref="en:Tablehead"/>
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Td"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Trace"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:Until"/>
        <xs:element ref="en:Value"/>
        <xs:element ref="en:While"/>
      </xs:choice>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Wählt die Kindelemente aus.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="limit">
        <xs:annotation>
          <xs:documentation>Die Anzahl der Kinder wird durch die Zahl begrenzt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="start">
        <xs:annotation>
          <xs:documentation>Der erste Eintrag der verarbeitet werden soll. Voreinstellung ist 1.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Frame">
    <xs:annotation>
      <xs:documentation>Um ein Objekt ein Rahmen zeichnen. Kann als Clipping-Pfad benutzt werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Box"/>
        <xs:element ref="en:Circle"/>
        <xs:element ref="en:Rule"/>
        <xs:element ref="en:Textblock"/>
        <xs:element ref="en:Barcode"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:Transformation"/>
        <xs:element ref="en:Frame"/>
      </xs:choice>
      <xs:attribute name="backgroundcolor">
        <xs:annotation>
          <xs:documentation>Hintergrundfarbe wenn das Objekt mit einer Farbe hinterlegt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom-right-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke unten rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top-right-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke oben rechts.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-top-left-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke oben links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="border-bottom-left-radius">
        <xs:annotation>
          <xs:documentation>Radius der Ecke unten links.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="framecolor">
        <xs:annotation>
          <xs:documentation>Die Farbe des Rahmens, wenn ein Rahmen ausgegeben wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rulewidth">
        <xs:annotation>
          <xs:documentation>Die Dicke des Rahmens, der um das Objekt gezogen wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>CSS Klasse für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="clip">
        <xs:annotation>
          <xs:documentation>Bestimmt, ob die Inhalte des Rahmens auf dessen Fläche begrenzt werden sollen, oder ob sie darüber hinaus ragen dürfen.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Die Inhalte werden an dessen Rändern des Rahmens abgeschnitten (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Die Inhalte nicht abschneiden.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Grid">
    <xs:annotation>
      <xs:documentation>Überschreibt das allgemeine Raster aus SetGrid.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Breite einer Rasterzelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="height">
        <xs:annotation>
          <xs:documentation>Höhe einer Rasterzelle.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="nx">
        <xs:annotation>
          <xs:documentation>Anzahl der Rasterzellen in horizontaler Richtung.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ny">
        <xs:annotation>
          <xs:documentation>Anzahl der Rasterzellen in vertikaler Richtung.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="dx">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Rasterzellen (horizontal)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="dy">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Rasterzellen (horizontal)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Group">
    <xs:annotation>
      <xs:documentation>Erzeugt einen virtuellen Bereich, in dem Elemente platziert werden können. Ausgegeben werden die Gruppen anschließend mit PlaceObject.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Contents"/>
        <xs:element ref="en:Grid"/>
      </xs:choice>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der anzulegenden Gruppe.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Groupcontents">
    <xs:annotation>
      <xs:documentation>Fügt den Inhalt der Gruppe ein (einer virtuellen Fläche)</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der Gruppe</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Hyphenation" type="xs:string">
    <xs:annotation>
      <xs:documentation>Erzeugt einen Trennvorschlag für die automatische Silbentrennung. Die Trennvorschläge werden dann benutzt, wenn die automatische Silbentrennung fehlerhaft trennt. Alle Trennstellen müssen mit eine Bindestrich (-) markiert werden (s. Beispiel).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="HSpace">
    <xs:annotation>
      <xs:documentation>Zwei Modi: bei einer gegebenen Breite wird ein Leerraum mit dieser Breite erzeugt.
      Ohne Breitenangabe: erzeugt einen horizontalen dehnbaren Leerraum. Der Leerraum hat die minimale Größe 0 und die maximale Größe »unendlich«. Sinnvoll nutzbar in einzeiligen Absätzen. Im Fließtext wird es vermutlich unerwartete Ergebnisse hervorrufen. Der Leerraum in einer Zeile verteilt sich normalerweise gleichmäßig über jeden Wortzwischenraum. Wenn ein
      HSpace enthalten ist, erhalten alle Leerräume die minimale Breite, nur der dehnbare Leerraum hat den überschüssigen Leerraum »angehäuft«.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="leader">
        <xs:annotation>
          <xs:documentation>Der Text, der anstelle des Leerraums angezeigt wird. Beispielsweise der Punkt (.).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leader-width">
        <xs:annotation>
          <xs:documentation>Abstand zwischen zwei Startpunkten des Texts in der Führungslinie.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="minwidth">
        <xs:annotation>
          <xs:documentation>Die (optionale) minimale Breite des eingefügten Leerraums.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Optionale Breite des Leerraums</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="I">
    <xs:annotation>
      <xs:documentation>Schaltet auf »Kursiv« um.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:A"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Color"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:NoBreak"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:URL"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Include">
    <xs:annotation>
      <xs:documentation>Toplevel Element für eingebundene Layoutdateien.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:DefineColor"/>
        <xs:element minOccurs="0" ref="en:DefineFontalias"/>
        <xs:element minOccurs="0" ref="en:DefineFontfamily"/>
        <xs:element minOccurs="0" ref="en:DefineTextformat"/>
        <xs:element minOccurs="0" ref="en:Hyphenation"/>
        <xs:element minOccurs="0" ref="en:LoadFontfile"/>
        <xs:element minOccurs="0" ref="en:Options"/>
        <xs:element minOccurs="0" ref="en:Pageformat"/>
        <xs:element minOccurs="0" ref="en:Pagetype"/>
        <xs:element minOccurs="0" ref="en:Record"/>
        <xs:element minOccurs="0" ref="en:SetGrid"/>
        <xs:element minOccurs="0" ref="en:SetVariable"/>
        <xs:element minOccurs="0" ref="en:Stylesheet"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
      </xs:sequence>
      <xs:attribute ref="xml:base"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Initial">
    <xs:annotation>
      <xs:documentation>Einige Zeichen erscheinen in einer größeren Schriftart am Absatzanfang.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="en:Value"/>
      </xs:sequence>
      <xs:attribute name="color">
        <xs:annotation>
          <xs:documentation>Setze die Farbe der Initiale. Voreingestellt ist Schwarz.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fontface" use="required">
        <xs:annotation>
          <xs:documentation>Wähle die Fontfamilie. Derzeit wird nur der Schnitt Regular verwendet.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-left">
        <xs:annotation>
          <xs:documentation>Fügt Leerraum links der Initiale ein.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-right">
        <xs:annotation>
          <xs:documentation>Fügt Leerraum right der Initiale ein.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="InsertPages">
    <xs:annotation>
      <xs:documentation>Fügt zuvor gespeicherte Seiten ein oder reserviert Raum für zukünftig erzeugte Seiten.
      Es gibt zwei Modi für diesen Befehl. Der erste Modus ist, dass zuvor mit SavePages erzeugte Seiten eingefügt werden. Der zweite Modus (»zukünftige Seiten«) reserviert Seiten im PDF, die später mit SavePages erzeugt werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der Name der gespeicherten Seiten.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pages">
        <xs:annotation>
          <xs:documentation>Anzahl der Seiten die freigehalten werden sollen. Funktioniert im »Modus zukünftige Seiten«.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Image">
    <xs:annotation>
      <xs:documentation>Bindet eine externe Grafik ein. Erlaubte Grafikformate sind PDF (.pdf), PNG (.png) und JPEG (.jpg). Siehe unten für die Einschränkung eingebundener PDF-Dateien.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="en:Value"/>
      </xs:sequence>
      <xs:attribute name="class">
        <xs:annotation>
          <xs:documentation>CSS Klasse für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="id">
        <xs:annotation>
          <xs:documentation>CSS id für dieses Element.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bleed">
        <xs:annotation>
          <xs:documentation>Soll das Bild um die Beschnittzugabe (Options) größer werden?</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="auto">
              <xs:annotation>
                <xs:documentation>Wenn das Bild die Papierkante berührt, vergrößere das Bild in diese Richtung.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Das Bild nicht vergrößern.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="clip">
        <xs:annotation>
          <xs:documentation>Wenn 'yes', dann behält das Bild sein Seitenverhältnis wenn Breite und Höhe gegeben ist. Um das Bild einzupassen, wird ein Teil abgeschnitten.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Seitenverhältnis beibehalten und linken und rechten oder oberen und unteren Rand abschneiden.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Bild verzerren, damit es in den vorgegebenen Rahmen passt.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="dpiwarn">
        <xs:annotation>
          <xs:documentation>Warnung ausgeben, wenn das Bild eine geringere Auflösung bekommt als angegeben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fallback">
        <xs:annotation>
          <xs:documentation>Der Dateiname für ein Ersatzbild, falls die Bilddatei nicht gefunden wurde. Wenn keiner angegeben wird, wird ein roter Platzhalter angezeigt.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="file">
        <xs:annotation>
          <xs:documentation>Dateiname des Bildes. Kann entweder eine Datei im Suchpfad sein, ein absoluter Dateiname, ein file-URI-Schema (z.B. file:///pfad/zum/bild.pdf) oder eine Quelle im Web (http/https).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="height">
        <xs:annotation>
          <xs:documentation>Bildhöhe. Angabe in der Form 'auto' (Voreinstellung, natürliche Bildbreite), Längenangabe wie '4cm' oder Zahl (Anzahl von Rasterspalten)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="page">
        <xs:annotation>
          <xs:documentation>Die Seitenzahl aus dem PDF. Voreinstellung ist 1.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="maxwidth">
        <xs:annotation>
          <xs:documentation>Die maximale Breite des Bilds. Nur, wenn clip="no". Angabe in absoluten Längen, Rasterzellen oder »100%« für volle Breite.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="maxheight">
        <xs:annotation>
          <xs:documentation>Die maximale Breite des Bilds. Nur, wenn clip="no". Angabe in absoluten Längen oder Anzahl Rasterzellen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="minwidth">
        <xs:annotation>
          <xs:documentation>Die minimale Breite des Bilds. Nur, wenn clip="no". Angabe in absoluten Längen, Rasterzellen oder »100%« für volle Breite.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="minheight">
        <xs:annotation>
          <xs:documentation>Die minimale Breite des Bilds. Nur, wenn clip="no". Angabe in absoluten Längen oder Anzahl Rasterzellen.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding">
        <xs:annotation>
          <xs:documentation>Setze Padding für alle Seiten.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-top">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand des Bildes (oben)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-bottom">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand des Bildes (unten)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-left">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand des Bildes (links)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="padding-right">
        <xs:annotation>
          <xs:documentation>Bestimmt den Innenabstand des Bildes (rechts)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rotate">
        <xs:annotation>
          <xs:documentation>Dreht das Bild in 90°-Schritten. Ein Winkel &gt; 0 dreht das Objekt im Uhrzeigersinn, ein Winkel &lt; 0 gegen den Uhrzeigersinn.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="visiblebox">
        <xs:annotation>
          <xs:documentation>Die PDF-Box, die den sichtbaren Bereich angibt des eingebundenen PDFs angibt. Voreinstellung ist »cropbox«.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="artbox">
              <xs:annotation>
                <xs:documentation>Nutze die artbox als sichtbaren Bereich. Diese Box ist normalerweise in PDF-Dateien nicht enthalten.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bleedbox">
              <xs:annotation>
                <xs:documentation>Nutze die bleedbox des eingebundenen PDFs.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="cropbox">
              <xs:annotation>
                <xs:documentation>Nutze die cropbox der eingebundenen PDF-Datei (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="mediabox">
              <xs:annotation>
                <xs:documentation>Nutze die mediabox der eingebundenen PDF-Datei. Das ist die größte Box.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="trimbox">
              <xs:annotation>
                <xs:documentation>Nutze die trimbox der eingebundenen PDF-Datei. Die trimbox ist die Papiergröße. Beispielsweise hat die trimbox eines A4-PDFs die Größe 210mm x 297mm.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="width">
        <xs:annotation>
          <xs:documentation>Bildbreite. Angabe in der Form 'auto' (Voreinstellung, natürliche Bildbreite), '100%' (Breite des aktuellen Platzierungsbereichs), Längenangabe wie '4cm' oder Zahl (Anzahl von Rasterspalten)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="imagetype">
        <xs:annotation>
          <xs:documentation>Setze den Konverter, der für das eingebettete Bild notwendig ist (sofern vorhanden).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Italic">
    <xs:annotation>
      <xs:documentation>Dient als Festlegung des Schriftschnittes, wenn Formatierung I (Kursiv) angegeben ist.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="fontface" use="required">
        <xs:annotation>
          <xs:documentation>Name der Schriftart, die für eine kursive Schrift benutzt werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="LoadDataset">
    <xs:annotation>
      <xs:documentation>Lädt eine Datensatzdatei, die in einem vorherigen Durchlauf des Publishers erzeugt wurde (Attribut name), oder eine reguläre XML-Datei (Attribut filename). Die »normale« Verarbeitung des Layoutregelwerks wird unterbrochen und mit dem Inhalt der Datensatzdatei fortgeführt. Nachdem die Verarbeitung der neu geladenen Datensatzdatei beendet ist, wird die Verarbeitung des Layoutregelwerks mit dem ursprünglichen Datensatz fortgesetzt. Eine ausführlichere Erläuterung findet sich im Abschnitt über automatisch generierte Verzeichnisse.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="name">
        <xs:annotation>
          <xs:documentation>Name der Dateisatzdatei. Beispiel: toc</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="filename">
        <xs:annotation>
          <xs:documentation>Name der XML-Datei, die geladen werden soll. Beispiel: meinedatei.xml.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="LoadFontfile">
    <xs:annotation>
      <xs:documentation>Der Befehl LoadFontfile weist einer physikalischen Schriftdatei einen internen Namen zu. Wird ein Zeichen in der Schriftart nicht gefunden, erzeugt das Programm eine Fehlermeldung, die mit Options ausgeschaltet werden kann. Alternativ dazu können auch Ersatzschriftarten (mit Fallback) angegeben werden, die nach den fehlenden Zeichen abgesucht werden.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="en:Fallback"/>
      </xs:sequence>
      <xs:attribute name="name" use="required">
        <xs:annotation>
          <xs:documentation>Der interne Name, unter dem die Schriftart im Element DefineFontfamily angesprochen wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="filename" use="required">
        <xs:annotation>
          <xs:documentation>Der Dateiname mit Endung, ohne Pfad.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="marginprotrusion">
        <xs:annotation>
          <xs:documentation>Angabe in Prozent, um wie viel die Zeichen -, . und , über den Rand herausragen. Stark abhängig von der benutzten Schriftart.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="oldstylefigures">
        <xs:annotation>
          <xs:documentation>Schalter, ob auf Mediävalziffern umgeschaltet wird (OpenType Feature »onum«).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Mediävalziffern benutzen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Normale Ziffern benutzen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="smallcaps">
        <xs:annotation>
          <xs:documentation>Schalter, ob die Schriftart als Kapitälchen dargestellt werden soll.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Kapitälchen benutzen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Nicht auf Kapitälchen umschalten (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="space">
        <xs:annotation>
          <xs:documentation>Natürliche Breite des Leerraums zwischen zwei Wörtern. Dieser darf um 30% gestreckt und um 10% gestaucht werden. Die Voreinstellung ist 25. Der Wert ist die Prozentangabe der Schriftgröße.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Layout">
    <xs:annotation>
      <xs:documentation>Das Layout-Element ist das Wurzelelement im Layoutregelwerk.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="en:AttachFile"/>
        <xs:element minOccurs="0" ref="en:AddSearchpath"/>
        <xs:element minOccurs="0" ref="en:Compatibility"/>
        <xs:element minOccurs="0" ref="en:DefineColor"/>
        <xs:element minOccurs="0" ref="en:DefineColorprofile"/>
        <xs:element minOccurs="0" ref="en:DefineFontalias"/>
        <xs:element minOccurs="0" ref="en:DefineFontfamily"/>
        <xs:element minOccurs="0" ref="en:DefineTextformat"/>
        <xs:element minOccurs="0" ref="en:Hyphenation"/>
        <xs:element minOccurs="0" ref="en:LoadFontfile"/>
        <xs:element minOccurs="0" ref="en:Loop"/>
        <xs:element minOccurs="0" ref="en:Options"/>
        <xs:element minOccurs="0" ref="en:Pageformat"/>
        <xs:element minOccurs="0" ref="en:Pagetype"/>
        <xs:element minOccurs="0" ref="en:PlaceObject"/>
        <xs:element minOccurs="0" ref="en:PDFOptions"/>
        <xs:element minOccurs="0" ref="en:Record"/>
        <xs:element minOccurs="0" ref="en:SetGrid"/>
        <xs:element minOccurs="0" ref="en:SetVariable"/>
        <xs:element minOccurs="0" ref="en:Stylesheet"/>
        <xs:element minOccurs="0" ref="en:Switch"/>
        <xs:element minOccurs="0" ref="en:Trace"/>
        <xs:element minOccurs="0" ref="en:While"/>
        <xs:element minOccurs="0" ref="en:Layout"/>
        <xs:element minOccurs="0" ref="en:Include"/>
      </xs:sequence>
      <xs:attribute name="version">
        <xs:annotation>
          <xs:documentation>Gibt die minimale Version des Publishers an. Format: 1.6.13. Die letzten Ziffern können weggelassen werden. Falls die erste oder die zweite Ziffer der Versionsangabe unterschiedlich ist, wird eine Warnung ausgegeben.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name">
        <xs:annotation>
          <xs:documentation>Name des Layouts. Hat keinen Einfluss auf das Layout.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Li">
    <xs:annotation>
      <xs:documentation>Listeneintrag in einer Aufzählungsliste (mit Aufzählungspunkten oder nummerierten Punkten).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:A"/>
        <xs:element ref="en:Action"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Color"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:NoBreak"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:URL"/>
        <xs:element ref="en:Value"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Loop">
    <xs:annotation>
      <xs:documentation>Wiederhole den Inhalt dieses Elements mehrfach.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Action"/>
        <xs:element ref="en:AddSearchpath"/>
        <xs:element ref="en:Attribute"/>
        <xs:element ref="en:B"/>
        <xs:element ref="en:Barcode"/>
        <xs:element ref="en:Bookmark"/>
        <xs:element ref="en:Br"/>
        <xs:element ref="en:Column"/>
        <xs:element ref="en:Columns"/>
        <xs:element ref="en:Copy-of"/>
        <xs:element ref="en:DefineColor"/>
        <xs:element ref="en:DefineFontfamily"/>
        <xs:element ref="en:DefineFontalias"/>
        <xs:element ref="en:DefineTextformat"/>
        <xs:element ref="en:Element"/>
        <xs:element ref="en:Fontface"/>
        <xs:element ref="en:ForAll"/>
        <xs:element ref="en:Frame"/>
        <xs:element ref="en:Group"/>
        <xs:element ref="en:HSpace"/>
        <xs:element ref="en:Hyphenation"/>
        <xs:element ref="en:I"/>
        <xs:element ref="en:Image"/>
        <xs:element ref="en:Initial"/>
        <xs:element ref="en:Include"/>
        <xs:element ref="en:InsertPages"/>
        <xs:element ref="en:Layout"/>
        <xs:element ref="en:Li"/>
        <xs:element ref="en:LoadDataset"/>
        <xs:element ref="en:LoadFontfile"/>
        <xs:element ref="en:Loop"/>
        <xs:element ref="en:Message"/>
        <xs:element ref="en:NewPage"/>
        <xs:element ref="en:NextFrame"/>
        <xs:element ref="en:NextRow"/>
        <xs:element ref="en:Options"/>
        <xs:element ref="en:Output"/>
        <xs:element ref="en:Pageformat"/>
        <xs:element ref="en:Paragraph"/>
        <xs:element ref="en:PlaceObject"/>
        <xs:element ref="en:PositioningArea"/>
        <xs:element ref="en:PositioningFrame"/>
        <xs:element ref="en:ProcessNode"/>
        <xs:element ref="en:SaveDataset"/>
        <xs:element ref="en:SavePages"/>
        <xs:element ref="en:SetGrid"/>
        <xs:element ref="en:SetVariable"/>
        <xs:element ref="en:SortSequence"/>
        <xs:element ref="en:Span"/>
        <xs:element ref="en:Sub"/>
        <xs:element ref="en:Sup"/>
        <xs:element ref="en:Switch"/>
        <xs:element ref="en:Table"/>
        <xs:element ref="en:TableNewPage"/>
        <xs:element ref="en:Tablehead"/>
        <xs:element ref="en:Tablerule"/>
        <xs:element ref="en:Td"/>
        <xs:element ref="en:Tr"/>
        <xs:element ref="en:Trace"/>
        <xs:element ref="en:U"/>
        <xs:element ref="en:Until"/>
        <xs:element ref="en:Value"/>
        <xs:element ref="en:While"/>
      </xs:choice>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Die Anzahl der Schleifendurchläufe. Muss eine Zahl oder in eine Zahl konvertierbar sein.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="variable">
        <xs:annotation>
          <xs:documentation>Wenn angegeben, wird die Nummer des Schleifendurchlaufs in dieser Variablen gespeichert, ansonsten in _loopcounter.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Makeindex">
    <xs:annotation>
      <xs:documentation>Eine Liste von Elementen sortieren und aufteilen um einen Index zu erzeugen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="pagenumber">
        <xs:annotation>
          <xs:documentation>Der Name des Attributs der die Seitenzahl enthält. Voreinstellung ist 'page'.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="section" use="required">
        <xs:annotation>
          <xs:documentation>Erzeuge ein XML-Element mit diesem Namen für jeden Abschnitt (Buchstabe) in dem Index.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Der XPath-Ausdruck der die Elementstruktur für den Index enthält ($Variable).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="sortkey" use="required">
        <xs:annotation>
          <xs:documentation>Der Name des Attributs das den Indexeintrag enthält (der sortiert werden soll).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Margin">
    <xs:annotation>
      <xs:documentation>Enthält die Angaben über den Seitenrand des zu druckbaren Bereich.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="left" use="required">
        <xs:annotation>
          <xs:documentation>Rand links</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="right" use="required">
        <xs:annotation>
          <xs:documentation>Rand rechts</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="top" use="required">
        <xs:annotation>
          <xs:documentation>Rand oben</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bottom" use="required">
        <xs:annotation>
          <xs:documentation>Rand unten</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Mark">
    <xs:annotation>
      <xs:documentation>Setzt eine unsichtbare Markierung in die Ausgabe. Das ist hilfreich, um die Seitenzahl zu bestimmen, auf der die Marke gelandet ist.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="append">
        <xs:annotation>
          <xs:documentation>Wenn 'yes', dann wird die aktuelle Seitenzahl an den letzten Wert angehängt (mit Komma separiert). Nützlich, um Seitenzahlen wie S. 3-5 im Index zu erstellen. Voreinstellung ist nein.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Die Seitenzahl an die vorherigen Werte anhängen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Den vorherigen Wert ersetzen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="select" use="required">
        <xs:annotation>
          <xs:documentation>Der Name des Markers, der gesetzt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pdftarget">
        <xs:annotation>
          <xs:documentation>Setze ein PDF-Ziel, das mit A referenziert werden kann.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes"/>
            <xs:enumeration value="no"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Message">
    <xs:annotation>
      <xs:documentation>Gibt eine Textmeldung auf der Konsole und in der Protokolldatei aus.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="en:Value"/>
        <xs:element ref="en:Element"/>
      </xs:choice>
      <xs:attribute name="select">
        <xs:annotation>
          <xs:documentation>Inhalt der Nachricht. Alternativ dazu: Kindelemente Wert innerhalb des Elements »Message«</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="error">
        <xs:annotation>
          <xs:documentation>Zusätzlich einen Fehler melden.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Fehler ausgeben.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Keinen Fehler ausgeben (Voreinstellung).</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="errorcode">
        <xs:annotation>
          <xs:documentation>Wenn ein Fehler ausgegeben wird, benutze diesen Fehlercode. Voreinstellung 1. Negative Werte sind für interne Zwecke reserviert.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="exit">
        <xs:annotation>
          <xs:documentation>Soll sich der Publisher beenden?</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="no">
              <xs:annotation>
                <xs:documentation>Der speedata Publisher erzeugt weiterhin die PDF-Datei</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="yes">
              <xs:annotation>
                <xs:documentation>Der speedata Publisher beendet sich ohne die PDF-Datei fertig zu stellen.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="NewPage">
    <xs:annotation>
      <xs:documentation>Beendet die Ausgabe auf der aktuellen Seite und erzeugt eine neue Seite. Diese wird nur angelegt, wenn später Text o.ä. ausgegeben wird.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="openon">
        <xs:annotation>
          <xs:documentation>Die nächste aktuelle Seite ist eine linke oder rechte Seite. Z.B. wenn auf Seite 1 ein Seitenwechsel erzwungen wird mit openon="right", dann wird Seite zwei leer und die nächste aktuelle Seite ist die Seite 3.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="left">
              <xs:annotation>
                <xs:documentation>Die nächsten Objekte werden auf einer linken Seite ausgegeben.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="right">
              <xs:annotation>
                <xs:documentation>Die nächsten Objekte werden auf einer rechten Seite ausgegeben.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="pagetype">
        <xs:annotation>
          <xs:documentation>Der Name des Seitentyps der für die nächste Seite genommen werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="skippagetype">
        <xs:annotation>
          <xs:documentation>Der Seitentyp der leeren Seite, falls sie eingefügt wird.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="NextFrame">
    <xs:annotation>
      <xs:documentation>Schaltet auf den nächsten freien Rahmen eines Platzierungsbereichs um.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="area" use="required">
        <xs:annotation>
          <xs:documentation>Name des Platzierungsbereich, in dem der nächste Rahmen gesucht werden soll.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="NextRow">
    <xs:annotation>
      <xs:documentation>Der virtuelle Cursor wird auf die nächste freie Zeile gesetzt.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
